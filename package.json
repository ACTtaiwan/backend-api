{
  "name": "ustw-backend-api",
  "version": "0.0.1",
  "private": true,
  "description": "US Taiwan Watch Backend APIs",
  "main": "handler.js",
  "scripts": {
    "start": "npm run rest",
    "dev": "export NODE_ENV=local && npm run rest",
    "rest": "nodemon --ignore './data' --exec node --inspect=9453 -r ts-node/register ./functions/server-rest.ts",
    "serverless": "node_modules/serverless/bin/serverless offline",
    "deploy-dev": "node_modules/serverless/bin/serverless deploy --stage dev",
    "deploy-prod": "node_modules/serverless/bin/serverless deploy --stage prod",
    "test": "export NODE_ENV=test && mocha --require ts-node/register --recursive \"**/_test/*.ts\"",
    "ts-node": "node node_modules/ts-node/dist/bin.js",
    "lint": "tslint --project .",
    "lintfix": "tslint --fix --project ."
  },
  "config": {
    "db_config": "remoteMongodb"
  },
  "keywords": [],
  "author": "hsuyachieh@gmail.com",
  "license": "ISC",
